@incollection{froehlich_longitudinal_2011,
	title = {A Longitudinal Study of Pressure Sensing to Infer Real-World Water Usage Events in the Home},
	rights = {©2011 Springer-Verlag {GmbH} Berlin Heidelberg},
	isbn = {978-3-642-21725-8 978-3-642-21726-5},
