@inproceedings{yano_textual_2012,
	location = {Stroudsburg, {PA}, {USA}},
	title = {Textual Predictors of Bill Survival in Congressional Committees},
	isbn = {978-1-937284-20-6},
	url = {http://dl.acm.org/citation.cfm?id=2382029.2382157},
	series = {{NAACL} {HLT} '12},
	abstract = {A U. S. Congressional bill is a textual artifact that must pass through a series of hurdles to become a law. In this paper, we focus on one of the most precarious and least understood stages in a bill's life: its consideration, behind closed doors, by a Congressional committee. We construct predictive models of whether a bill will survive committee, starting with a strong, novel baseline that uses features of the bill's sponsor and the committee it is referred to. We augment the model with information from the contents of bills, comparing different hypotheses about how a committee decides a bill's fate. These models give significant reductions in prediction error and highlight the importance of bill substance in explanations of policy-making and agenda-setting.},
	pages = {793--802},
	booktitle = {Proceedings of the 2012 Conference of the North American Chapter of the Association for Computational Linguistics: Human Language Technologies},
	publisher = {Association for Computational Linguistics},
	author = {Yano, Tae and Smith, Noah A. and Wilkerson, John D.},
	urldate = {2015-12-20},
