	file = {Full Text PDF:/Users/johnjnay/Library/Application Support/Zotero/Profiles/qzdwua0k.default/zotero/storage/E32ITI4Q/Rand et al. - 2009 - Positive Interactions Promote Public Cooperation.pdf:application/pdf;Snapshot:/Users/johnjnay/Library/Application Support/Zotero/Profiles/qzdwua0k.default/zotero/storage/375G72NJ/1272.html:text/html}
