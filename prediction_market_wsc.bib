@Article{zotero-null-553,
  Title                    = {Dominant strategy implementation with a convex product space of valuations},
  Author                   = {Cuff, Katherine and Hong, Sunghoon and Schwartz, Jesse A. and Wen, Quan and Weymark, John A.},
  Journal                  = {Social Choice and Welfare},
