@article{Halldorsson2003,
  title = {Approximability results for stable marriage problems with ties},
  volume = {306},
  issn = {0304-3975},
  url = {http://www.sciencedirect.com/science/article/pii/S0304397503003219},
  doi = {10.1016/S0304-3975(03)00321-9},
  abstract = {We consider instances of the classical stable marriage problem in which persons may include ties in their preference lists. We show that, in such a setting, strong lower bounds hold for the approximability of each of the problems of finding an egalitarian, minimum regret and sex-equal stable matching. We also consider stable marriage instances in which persons may express unacceptable partners in addition to ties. In this setting, we prove that there are constants \ensuremath{\delta},\ensuremath{{\delta}{'}} such that each of the problems of approximating a maximum and minimum cardinality stable matching within factors of \ensuremath{\delta},\ensuremath{{\delta}{'}} (respectively) is NP-hard, under strong restrictions. We also give an approximation algorithm for both problems that has a performance guarantee expressible in terms of the number of lists with ties. This significantly improves on the best-known previous performance guarantee, for the case that the ties are sparse. Our results have applications to large-scale centralized matching schemes.},
  timestamp = {2015-03-23 14:04:10},
  number = {1{\textendash}3},
  urldate = {2015-03-23},
  journal = {Theoretical Computer Science},
  author = {Halld{\'o}rsson, Magn{\'u}s M. and Irving, Robert W. and Iwama, Kazuo and Manlove, David F. and Miyazaki, Shuichi and Morita, Yasufumi and Scott, Sandy},
  month = sep,
  year = {2003},
  keywords = {Approximation algorithm,Inapproximability results,Stable marriage problem,Ties,Unacceptable partners},
  pages = {431--447},
  file = {Halldórsson et al_2003_Approximability results for stable marriage problems with ties.pdf:/Users/martinvanderlinden/Copy/Literature/storage/DJP4NM7J/Halldórsson et al_2003_Approximability results for stable marriage problems with ties.pdf:application/pdf;ScienceDirect Snapshot:/Users/martinvanderlinden/Copy/Literature/storage/6EF6BV2G/S0304397503003219.html:text/html}
