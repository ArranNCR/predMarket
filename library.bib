@misc{Echenique2009,
  type = {Report or Paper},
  title = {Clearinghouses for Two-Sided Matching: An Experimental Study},
  shorttitle = {Clearinghouses for Two-Sided Matching},
  url = {http://resolver.caltech.edu/CaltechAUTHORS:20101008-102338754},
  abstract = {We study the performance of two-sided matching clearinghouses in the laboratory.  Our experimental design mimics the Gale-Shapley (1962) mechanism, utilized to match hospitals and interns, schools and pupils, etc., with an array of preference profiles. Several insights come out of our analysis. First, only 48\% of the observed match outcomes are fully stable. Furthermore, among those markets ending at a stable outcome, a large majority culminates in the best stable matching for the receiving-side. Second, contrary to the theory, participants on the receiving-side of the algorithm rarely truncate their true preferences. In fact, it is the proposers who do not make offers in order of their preference, frequently skipping potential partners. Third, market characteristics affect behavior and outcomes: both the cardinal representation and the span of the core influence whether outcomes are stable or close to stable, as well as the number of turns it takes markets to converge to the final outcome.},
  timestamp = {2015-03-30 21:39:41},
  urldate = {2015-03-30},
  author = {Echenique, Federico and Wilson, Alistair J. and Yariv, Leeat},
  month = dec,
