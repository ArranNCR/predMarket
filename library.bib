@article{Tseng2010a,
  title = {Statistical properties of agent-based models in markets with continuous double auction mechanism},
  volume = {389},
  issn = {0378-4371},
  url = {http://www.sciencedirect.com/science/article/pii/S0378437109010486},
  doi = {10.1016/j.physa.2009.12.034},
  abstract = {Real world markets display power-law features in variables such as price fluctuations in stocks. To further understand market behavior, we have conducted a series of market experiments on our web-based prediction market platform which allows us to reconstruct transaction networks among traders. From these networks, we are able to record the degree of a trader, the size of a community of traders, the transaction time interval among traders and other variables that are of interest. The distributions of all these variables show power-law behavior. On the other hand, agent-based models have been proposed to study the properties of real financial markets. We here study the statistical properties of these agent-based models and compare them with the results from our web-based market experiments. In this work, three agent-based models are studied, namely, zero-intelligence (ZI), zero-intelligence-plus (ZIP) and Gjerstad{\textendash}Dickhaut (GD). Computer simulations of variables based on these three agent-based models were carried out. We found that although being the most naive agent-based model, ZI indeed best describes the properties observed in real markets. Our study suggests that the basic ingredient to produce the observed properties from real world markets could in fact be the result of a continuously evolving dynamical system with basic features similar to the ZI model.},
  timestamp = {2015-02-02 16:52:18},
  number = {8},
  urldate = {2015-01-19},
  journal = {Physica A: Statistical Mechanics and its Applications},
  author = {Tseng, Jie-Jun and Lin, Chih-Hao and Lin, Chih-Ting and Wang, Sun-Chong and Li, Sai-Ping},
  month = apr,
  year = {2010},
  keywords = {Agent-based simulation,Complex networks,Continuous double action,Econophysics,Financial stylized facts},
  pages = {1699--1707},
  file = {ScienceDirect Snapshot:/Users/martinvanderlinden/Copy/Literature/storage/T5CKTPMZ/S0378437109010486.html:text/html;Snapshot:/Users/martinvanderlinden/Copy/Literature/storage/X2ZSU7MF/S0378437109010486.html:text/html;Tseng et al_2010_Statistical properties of agent-based models in markets with continuous double.pdf:/Users/martinvanderlinden/Copy/Literature/storage/NKIAJGX5/Tseng et al_2010_Statistical properties of agent-based models in markets with continuous double.pdf:application/pdf}
}

