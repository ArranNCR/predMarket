@article{Jumadinova2011a,
  title = {A multi-agent system for analyzing the effect of information on prediction markets},
  volume = {26},
  copyright = {Copyright {\textcopyright} 2011 Wiley Periodicals, Inc.},
  issn = {1098-111X},
  url = {http://onlinelibrary.wiley.com/doi/10.1002/int.20471/abstract},
  doi = {10.1002/int.20471},
  abstract = {Prediction markets have been shown to be a useful tool for forecasting the outcome of future events by aggregating public opinion about the event's outcome. In this paper, we investigate an important aspect of prediction markets{\textemdash}the effect of different information-related parameters on the behavior of the traders in the market. We have developed a multi-agent based system that incorporates different information-related aspects including the arrival rate of information, the reliability of information, the penetration or accessibility of information among the different traders, and the perception or impact of information by the traders. We have performed extensive simulations of our agent-based prediction market for analyzing the effect of information-related parameters on the traders' behaviors expressed through their trading prices, and compared our agents' strategies with another agent-based pricing strategy used in prediction markets called the zero intelligence strategy. Our results show that information-related parameters have a significant impact on traders' beliefs about event outcomes, and, frequent, reliable information about events improves the utilities that the traders receive. Overall, our work provides a better understanding of the effect of information on the operation of prediction markets and on the strategies used by the traders in the market. {\textcopyright} 2011 Wiley Periodicals, Inc.},
  language = {en},
  timestamp = {2015-02-02 16:54:49},
  number = {5},
  urldate = {2015-01-19},
  journal = {International Journal of Intelligent Systems},
  author = {Jumadinova, Janyl and Dasgupta, Prithviraj},
  year = {2011},
